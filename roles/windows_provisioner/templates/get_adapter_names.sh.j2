#!/bin/bash
# Check if exactly one argument is provided
if [ "$#" -ne 1 ]; then
  echo "Usage: $0 <name>"
  exit 1
fi

# Assign the first argument to a variable
MAC=$1

adapter_data = $(getmac /v /FO csv /NH | grep "$MAC")
echo $adapter_data | awk -F, '{gsub(/^\"/, "VZWI", $1); gsub(/\"$/, "VZWI", $1); print $1}'